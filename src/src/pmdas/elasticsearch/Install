# Install the ElasticSearch PMDA
#

. $PCP_DIR/etc/pcp.env
. $PCP_SHARE_DIR/lib/pmdaproc.sh

iam=elasticsearch
domain=108
python_opt=true
daemon_opt=false

pmdaSetup
pmdaInstall
exit
